@{
    Layout = "~/Views/Shared/FixtureMaster.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <title>Details</title>
</head>
<body>
    @*<ul class="breadcrumb">
            <li>您好，欢迎来到工夹具智能管理系统----进出库>>@Html.ActionLink("出库管理", "Index") / @Html.ActionLink("入库管理", "InIndex")>>出入库详情</li>
        </ul>*@
    <h4><span class="glyphicon  glyphicon-record " style=" position:relative ;height  :20px;top :3px;" /> 当前位置: @Html.ActionLink("首页", "Index", "Home", new { }, new { style = "color:blue;" })>><a class="panel-title collapsed" style=" color :blue ;font-size :18px;" data-toggle="collapse" data-parent="#panel-756431" href="#panel-element-21442" id="cgrk">进出库</a>>>@Html.ActionLink("信息查询", "SearchInoutMessage", new { }, new { style = "color:blue;" })>>出入库信息详情</h4>
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">用户详细信息</h3>
        </div>
        <div class="panel-body" style="text-align:center;">
            <table class="table table-bordered table-striped">
                <tr>
                    <td>夹具实体编号</td>
                    <td>@Html.DisplayFor(model => model.ItemID)</td>
                </tr>
                <tr>
                    <td>经手人员姓名</td>
                    <td>@Html.DisplayFor(model => model.ViaUserName)</td>
                </tr>
                <tr>
                    <td>进/出库状态</td>
                    <td>@Html.DisplayFor(model => model.State)</td>
                </tr>
                <tr>
                    <td>生产线编码</td>
                    <td>@Html.DisplayFor(model => model.LineID)</td>
                </tr>
                <tr>
                    <td>生产线名称</td>
                    <td>@Html.DisplayFor(model => model.LineName)</td>
                </tr>
                <tr>
                    <td>夹具代码</td>
                    <td>@Html.DisplayFor(model => model.Code)</td>
                </tr>
                <tr>
                    <td>夹具名称</td>
                    <td>@Html.DisplayFor(model => model.Name)</td>
                </tr>
                <tr>
                    <td>存放库位</td>
                    <td>@Html.DisplayFor(model => model.Location)</td>
                </tr>
                <tr>
                    <td>夹具模组</td>
                    <td>@Html.DisplayFor(model => model.Model)</td>
                </tr>
                <tr>
                    <td>夹具料号</td>
                    <td>@Html.DisplayFor(model => model.PartNo)</td>
                </tr>
                <tr>
                    <td>所属大类编码</td>
                    <td>@Html.DisplayFor(model => model.FamilyID)</td>
                </tr>
                <tr>
                    <td>所属大类名称</td>
                    <td>@Html.DisplayFor(model => model.FamilyName)</td>
                </tr>
                <tr>
                    <td>创建日期</td>
                    <td>@Html.DisplayFor(model => model.AddDate)</td>
                </tr>
                <tr>
                    <td>创建人员编码</td>
                    <td>@Html.DisplayFor(model => model.AddUserID)</td>
                </tr>
                <tr>
                    <td>创建人员姓名</td>
                    <td>@Html.DisplayFor(model => model.AddUserName)</td>
                </tr>
                <tr>
                    <td>审核日期</td>
                    <td>@Html.DisplayFor(model => model.ReviewDate)</td>
                </tr>
                <tr>
                    <td>审核人员编码</td>
                    <td>@Html.DisplayFor(model => model.ReviewUserID)</td>
                </tr>
                <tr>
                    <td>审核人员姓名</td>
                    <td>@Html.DisplayFor(model => model.ReviewUserName)</td>
                </tr>
            </table>
            <ul class="pager">
                <li class="edit">@Html.ActionLink("修改", "Edit", new { id = Model.ItemID })</li>
                <li class="index">@Html.ActionLink("返回", "SearchInoutMessage")</li>
            </ul>
        </div>
    </div>
</body>
</html>
