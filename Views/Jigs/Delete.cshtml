<div class="panel panel-primary">
    <div class="panel-heading">
        <h5 class="panel-title">部门名称</h5>
    </div>
    <div class="panel-body" style="text-align:center;">
        <table class="table table-bordered table-striped">
            <tr>
                <td>夹具代码</td>
                <td>@Html.DisplayFor(model => model.Code)</td>
            </tr>
            <tr>
                <td>夹具名称</td>
                <td>@Html.DisplayFor(model => model.Name)</td>
            </tr>
            <tr>
                <td>所属大类编码</td>
                <td>@Html.DisplayFor(model => model.FamilyID)</td>
            </tr>
            <tr>
                <td>所属大类名称</td>
                <td>@Html.DisplayFor(model => model.FamilyName)</td>
            </tr>
            <tr>
                <td>夹具模组</td>
                <td>@Html.DisplayFor(model => model.Model)</td>
            </tr>
            <tr>
                <td>夹具料号</td>
                <td>@Html.DisplayFor(model => model.PartNo)</td>
            </tr>
            <tr>
                <td>工作部门编码</td>
                <td>@Html.DisplayFor(model => model.WorkcellID)</td>
            </tr>
            <tr>
                <td>工作部门名称</td>
                <td>@Html.DisplayFor(model => model.WorkcellName)</td>
            </tr>
            <tr>
                <td>用途</td>
                <td>@Html.DisplayFor(model => model.UsedFor)</td>
            </tr>
            <tr>
                <td>数量</td>
                <td>@Html.DisplayFor(model => model.UPL)</td>
            </tr>
            <tr>
                <td>责任人编码</td>
                <td>@Html.DisplayFor(model => model.OwnerID)</td>
            </tr>
            <tr>
                <td>责任人姓名</td>
                <td>@Html.DisplayFor(model => model.OwnerName)</td>
            </tr>
            <tr>
                <td>保养点检周期</td>
                <td>@Html.DisplayFor(model => model.PMPeriod)</td>
            </tr>
            <tr>
                <td>创建日期</td>
                <td>@Html.DisplayFor(model => model.AddDate)</td>
            </tr>
            <tr>
                <td>创建人编码</td>
                <td>@Html.DisplayFor(model => model.AddUserID)</td>
            </tr>
            <tr>
                <td>创建人姓名</td>
                <td>@Html.DisplayFor(model => model.AddUserName)</td>
            </tr>
            <tr>
                <td>修改日期</td>
                <td>@Html.DisplayFor(model => model.EditDate)</td>
            </tr>
            <tr>
                <td>修改人编码</td>
                <td>@Html.DisplayFor(model => model.EditUserID)</td>
            </tr>
            <tr>
                <td>修改人姓名</td>
                <td>@Html.DisplayFor(model => model.EditUserName)</td>
            </tr>
            <tr>
                <td>备注</td>
                <td>@Html.DisplayFor(model => model.Remark)</td>
            </tr>
        </table>
        <div class="alert alert-warning">您确定要<strong> 删除 </strong>所选工夹具吗？</div>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <div class=" btn-group ">
                <input type="submit" value="删除" class="btn btn-danger" />
                <button class="btn btn-default" style="float:right">@Html.ActionLink("返回", "Index")</button>
            </div>
        }

    </div>
</div>

