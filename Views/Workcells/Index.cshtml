@model IEnumerable<Fixture02.Models.Workcell>

@using PagedList;
@using PagedList.Mvc;

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/FixtureMaster.cshtml";
}

<h2>部门管理</h2>

<p>
    @Html.ActionLink("新增", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WorkcellID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkcellName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
